
@{
    Layout = null;
}

@foreach (var item in Model)
{
<tr>
    <td>@item.Id</td>
    <td>@item.Nombre</td>
    <td>@item.Tipo</td>
    <td>
        <img src="~/Images/@item.Image" width="50" />
    </td>
    <td>
        <a class="btn btn-success" href="/Pokemon/CapturarPokemon?IdPokemon=@item.Id">Capturar</a>
    </td>
</tr>

}
